  skos:prefLabel "Graafschap "@nl ;
.
hg:Departement
  rdf:type hg:PitType ;
  rdfs:label "Departement"@nl ;
  skos:prefLabel "Departement"@nl ;
  skos:prefLabel "DÃ©partements"@fr ;
