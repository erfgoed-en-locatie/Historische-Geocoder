hg:Heerlijkheid
  rdf:type hg:PitType ;
  rdfs:label "Heerlijkheid "@nl ;
  skos:prefLabel "Heerlijkheid "@nl ;
.
hg:Municipality
  rdf:type hg:PitType ;
  rdfs:label "Gemeente"@nl ;
